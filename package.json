{
  "name": "tut-ext",
  "displayName": "tut-ext",
  "description": "tutorial of how to make an extension",
  "version": "0.0.1",
  "icon": "task.png",
  "publisher": "panzerstadt",
  "engines": {
    "vscode": "^1.42.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "taskOutline",
          "name": "Task Runner"
        }
      ]
    },
    "commands": [
      {
        "command": "taskOutline.executeTask",
        "title": "Execute"
      }
    ]
  },
  "keywords": [
    "tutorial",
    "tasks"
  ],
  "galleryBanner": {
    "color": "#FF3366"
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.42.0",
    "eslint": "^6.8.0",
    "@typescript-eslint/parser": "^2.18.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  }
}
